<div class="row">
	<div class="col s12">
		<ul class="tabs tabs-fixed-width">
			<li class="tab"><a href="#" class="active">Pikete</a></li>
			<li class="tab"><a href="#" onclick="apretaste.send({command: 'DIAMANTE RIFA'})">Rifa</a></li> 
		</ul>
	</div>
</div>

<div class="row">
	<% people.forEach(function(item){ %>
		<div class="col s4 m2">
			<div class="card user-card center-align">
				<div class="content">
					<div class="person-avatar circle" face="<%= item.avatar %>" color="<%= item.avatarColor %>" size="40" online="1"></div>
					<span class="<%= item.gender %>">@<%= short(item.username) %></span>
					<span class="chip tiny clear spacing"><i class="fas fa-bolt"></i> <%= item.experience %></span>
					<span class="chip tiny clear"><i class="fas fa-trophy"></i> #<%= item.rank %></span>
				</div>
				<div class="action grey lighten-2 green-text">
					<a href="#!"><i class="material-icons" onclick="apretaste.send({command:'PERFIL', data:{'username':'<%= item.username %>'}})">person</i></a>
				</div>
			</div>
		</div>
	<% }) %>
</div>
